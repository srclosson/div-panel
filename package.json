{
  "name": "stephanieclosson-div-panel",
  "version": "1.2.3",
  "description": "Create your own panel with html + css and javascript",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "e2e": "yarn exec cypress install && yarn exec grafana-e2e run",
    "e2e:update": "yarn exec cypress install && yarn exec grafana-e2e run --update-screenshots",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn run lint --fix",
    "server": "docker-compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit"
  },
  "author": "srclosson@gmail.com",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "7.25.7",
    "@grafana/e2e": "11.0.6",
    "@grafana/e2e-selectors": "11.2.2",
    "@grafana/eslint-config": "7.0.0",
    "@grafana/tsconfig": "2.0.0",
    "@swc/core": "1.7.26",
    "@swc/helpers": "0.5.13",
    "@swc/jest": "0.2.36",
    "@testing-library/jest-dom": "6.5.0",
    "@testing-library/react": "16.0.1",
    "@types/jest": "29.5.13",
    "@types/lodash": "4.17.10",
    "@types/node": "22.7.4",
    "@types/prop-types": "15.7.13",
    "@types/react": "18.3.11",
    "@cypress/request": "3.0.5",
    "@types/underscore": "1.11.15",
    "acorn": "8.12.1",
    "console-feed": "3.6.0",
    "copy-webpack-plugin": "12.0.2",
    "css-loader": "7.1.2",
    "eslint-webpack-plugin": "4.2.0",
    "fork-ts-checker-webpack-plugin": "9.0.2",
    "glob": "11.0.0",
    "handlebars": "4.7.8",
    "identity-obj-proxy": "3.0.0",
