    "@types/react-router-dom": "^5.2.0",
    "@types/testing-library__jest-dom": "5.14.8",
    "@types/underscore": "1.11.15",
    "acorn": "8.12.1",
    "console-feed": "3.6.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "debug": "4.3.7",
